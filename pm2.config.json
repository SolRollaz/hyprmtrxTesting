{
  "apps": [
    {
      "name": "auth-api",
      "script": "./api/auth/index.js",
      "watch": false,
      "cwd": "./",
      "node_args": "--enable-source-maps",
      "env": {
        "NODE_ENV": "production",
        "PORT": 4000
      },
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "error_file": "./logs/err.log",
      "out_file": "./logs/out.log",
      "merge_logs": true
    }
  ]
}
